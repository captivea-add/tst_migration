<?xml version="1.0" encoding="UTF-8" ?>
<odoo noupdate="1">

    <record model="res.users" id="base.user_admin">
        <field name="company_id" ref="cap_settings.company_tosa"/>
        <field name="company_ids" eval="[(6, 0, [ref('cap_settings.company_tosa'), ref('cap_settings.company_traction'), ref('cap_settings.company_puissance')])]"/>
        <field name="lang">fr_FR</field>
    </record>

    <record model="res.users" id="user_generic">
        <field name="login">generic</field>
        <field name="company_id" ref="company_tosa"/>
        <field name="company_ids" eval="[(6, 0, [ref('cap_settings.company_tosa'), ref('cap_settings.company_traction'), ref('cap_settings.company_puissance')])]"/>
        <field name="notification_type">inbox</field>
        <field name="odoobot_state">not_initialized</field>
        <field name="partner_id" ref="partner_generic"/>
        <field name="lang">fr_FR</field>
        <field name="tz">Europe/Paris</field>
    </record>
</odoo>