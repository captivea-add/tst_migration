<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <!-- Survey for consignment -->
    <record id="survey_electrical_consignment" model="survey.survey">
        <field name="access_mode">public</field>
        <field name="users_login_required">1</field>
        <field name="category">default</field>
        <field name="passing_score">100</field>
        <field name="questions_layout">one_page</field>
        <field name="questions_selection">all</field>
        <field name="scoring_type">no_scoring</field>
        <field name="state">open</field>
        <field name="title">Electrical Consignment</field>
    </record>

